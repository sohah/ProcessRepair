  p19 = (((Up_Separation_s < alim_res_s) and (Down_Separation_s >= Up_Separation_s)) => (result_alt_sep_test_s < 2));

  p20 = (((alim_res_s >= 400) and (result_alt_sep_test_s >= 0)) and ((result_alt_sep_test_s <= 2) and (alim_res_s <= 740)));

  p21 = (((Up_Separation_s < alim_res_s) and (alim_res_s < 400)) => (result_alt_sep_test_s <> 1));

  tight = (p21 => p4);

