  p20 : bool;
  p21 : bool;
  tight : bool;
let
  --%MAIN;
  p1 = (((Up_Separation_s < alim_res_s) and (Down_Separation_s < alim_res_s)) => (result_alt_sep_test_s <> 2));

  p2 = (((Up_Separation_s < alim_res_s) and (Down_Separation_s >= alim_res_s)) => (result_alt_sep_test_s <> 1));

  p4 = (((Up_Separation_s >= alim_res_s) and (Down_Separation_s < alim_res_s)) => (result_alt_sep_test_s <> 2));

  p5 = (((result_alt_sep_test_s <= 2) xor (result_alt_sep_test_s < 0)) and ((alim_res_s = 400) xor (alim_res_s > 499)));

  p6 = (((Up_Separation_s < alim_res_s) xor (Down_Separation_s >= alim_res_s)) => (result_alt_sep_test_s <= 2));

  p7 = (((Up_Separation_s < alim_res_s) => (Down_Separation_s >= alim_res_s)) => (result_alt_sep_test_s <= 2));

  p8 = ((false xor (Down_Separation_s >= alim_res_s)) => (result_alt_sep_test_s <> 1));

  p9 = (((Up_Separation_s < alim_res_s) and (Down_Separation_s <> alim_res_s)) => (result_alt_sep_test_s < 2));

