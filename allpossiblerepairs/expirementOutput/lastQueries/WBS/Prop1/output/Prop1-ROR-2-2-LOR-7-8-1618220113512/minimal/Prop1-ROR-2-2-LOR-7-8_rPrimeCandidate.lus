node main(
  pedal_r : int;
  autoBreak_r : bool;
  skid_r : bool;
  NormalPressure_r : int;
  AltPressure_r : int;
  Sys_Mode : int
) returns (
  p1 : bool
);
let
  --%MAIN;
  p1 = ((((pedal_r = 5) xor (pedal_r >= 1)) and (not skid_r)) => ((AltPressure_r = 0) and (NormalPressure_r > 0)));

  --%PROPERTY p1;

tel;
