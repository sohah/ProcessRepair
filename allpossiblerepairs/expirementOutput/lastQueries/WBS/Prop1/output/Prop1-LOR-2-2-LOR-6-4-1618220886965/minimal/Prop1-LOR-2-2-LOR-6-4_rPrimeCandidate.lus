node main(
  pedal_r : int;
  autoBreak_r : bool;
  skid_r : bool;
  NormalPressure_r : int;
  AltPressure_r : int;
  Sys_Mode : int
) returns (
  p1 : bool
);
let
  --%MAIN;
  p1 = ((((pedal_r <= 1) xor (pedal_r >= 3)) xor (not skid_r)) => ((AltPressure_r = 0) and (AltPressure_r = NormalPressure_r)));

  --%PROPERTY p1;

tel;
